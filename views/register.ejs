<body>

  <div class="limiter">
    <div class="container-login100">
      <div class="wrap-login100">
        <form method="post" class="login100-form validate-form">
          <span class="login100-form-logo">
            <img src="/public/images/logo - dark.png">
          </span>

          <span class="login100-form-title p-b-34 p-t-27 glow">
            Register
          </span>

          <div class="wrap-input100 validate-input" data-validate="Enter Full Name">
            <input class="input100" type="text" name="fullname" placeholder="Full Name">
            <span class="focus-input100" data-placeholder="&#xf207;"></span>
          </div>

          <div class="wrap-input100 validate-input" data-validate="Enter username">
            <input class="input100" type="text" name="username" placeholder="Username">
            <span class="focus-input100" data-placeholder="&#xf207;"></span>
          </div>

          <div class="wrap-input100 validate-input" data-validate="Enter password">
            <input class="input100" type="password" name="password" placeholder="Password">
            <span class="focus-input100" data-placeholder="&#xf191;"></span>
          </div>

          <div class="container-login100-form-btn glow p-t-20">
            <button type="button" class="text-black" onclick="location.href='/login'"><i
                class="bi bi-arrow-left"></i></button>
            <div class="mx-3"></div>
            <input type="submit" class="login100-form-btn" value="Register">
          </div>
        </form>
      </div>
    </div>
  </div>


  <div id="dropDownSelect1"></div>

